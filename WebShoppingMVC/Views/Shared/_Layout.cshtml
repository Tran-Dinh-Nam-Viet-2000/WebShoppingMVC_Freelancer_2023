        <div class="jumper">
            <div></div>
            <div></div>
            <div></div>
        </div>
    </div>
    <!-- ***** Preloader End ***** -->
    <!-- ***** Header Area Start ***** -->
    <header class="header-area header-sticky">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <nav class="main-nav">
                        <!-- ***** Logo Start ***** -->
                        <a asp-controller="Home" asp-action="Index" class="logo">
                            <img src="/assets/images/logo.png">
                        </a>
                        <!-- ***** Logo End ***** -->
                        <!-- ***** Menu Start ***** -->
                        <ul class="nav">
                            <li class="scroll-to-section"><a href="#top" class="active">Home</a></li>
                            <li class="scroll-to-section"><a href="#men">Shirt</a></li>
                            <li class="scroll-to-section"><a href="#women">Pants</a></li>
                            <li class="scroll-to-section"><a href="#kids">Accessory</a></li>
                            <li class="scroll-to-section"><a href="#kids">About Us</a></li>
                            <li class="submenu">
                                <a href="javascript:;">Profile</a>
                                <ul>
                                    <li><a href="#">My Profile</a></li>
                                    <li><a asp-controller="Authentication" asp-action="Login">Login</a></li>
